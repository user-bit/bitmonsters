.action-add
  position: relative
  width: 140px
  cursor pointer
  background: #5369f8
  color #ffffff
  border-radius: 4px
  padding: 5px 15px 6px
  display: flex
  align-items center
  transition: color 0.15s ease-in-out,background-color 0.15s ease-in-out,border-color 0.15s ease-in-out,box-shadow 0.15s ease-in-out
  &:hover
    background-color: #2e49f7
    border-color: #223ef6
  &:active
    box-shadow: 0 0 0 0.15rem rgba(109,128,249,.5)
    background-color: #2e49f7
    border-color: #223ef6
  a
    color #ffffff
    text-decoration none
    line-height 1
    font-size: 17px
    position: absolute
    left: 0
    top: 0
    bottom: 0
    right: 0
    display: flex
    align-items: center
    justify-content: flex-end
    padding-right: 20px
  svg
    width: 24px
    height: 24px
    color: #fff
    margin-right: 4px

