.params-group-list
  display: flex
  flex-wrap: wrap
  margin 0 -10px

.params-group
  width: calc(25% - 20px)
  margin: 0 10px
  margin-bottom: 60px
  &__name
    font-size: 16px
    font-weight: 500
    margin-bottom: 8px
  &__list
    height: 300px
    overflow: auto
    padding-left: 5px
    margin-left: -4px
    padding-top: 5px
    position relative
