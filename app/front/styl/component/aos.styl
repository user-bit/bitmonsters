.split-line {
  overflow: hidden;
}
.split-line div {
  transform: translate(30px, 340%) rotate(30deg);
  white-space: nowrap;
  box-sizing: border-box;
}
.js-animated-line {
  overflow: hidden;
}
.js-animated-line > div {
  transform: translate(30px, 340%) rotate(30deg);
}

